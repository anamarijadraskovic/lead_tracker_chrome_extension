{
    "manifest_version": 3,
    "version": "2.0",
    "name": "Track anything",
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
 }
}
